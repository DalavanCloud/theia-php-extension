{
  "name": "@theia/php",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "data",
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "next",
    "@theia/monaco": "next"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "postinstall": "composer install",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "publish:latest": "lerna publish --registry=https://registry.npmjs.org/ --exact --skip-git",
	"publish:next": "lerna publish --registry=https://registry.npmjs.org/ --exact --canary=next --npm-tag=next --force-publish --skip-git --yes"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/theia-php-frontend-module",
      "backend": "lib/node/theia-php-backend-module"
    }
  ]
}