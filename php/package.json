{
  "name": "@theia/php",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "data",
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "next",
    "@theia/monaco": "next"
  },
  "devDependencies": {
    "lerna": "2.4.0",
    "rimraf": "latest",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "postinstall": "composer install",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/theia-php-frontend-module",
      "backend": "lib/node/theia-php-backend-module"
    }
  ]
}
